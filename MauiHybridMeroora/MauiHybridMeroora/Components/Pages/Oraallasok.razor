@page "/oraallasok"
@using MauiHybridMeroora.mvvm.viewmodel
@inject MeroViewModel vm

<h3 class="text-center font-bold">Óraállások</h3>

    @foreach(var oraallas in vm.Oraallasok)
    {
        <Oraallas @key="oraallas.Id" OraAllas="@oraallas" />
    }


@code {
    protected override void OnInitialized()
    {
        vm.OnChange += StateHasChanged;
    }

    public void Dispose()
    {
        vm.OnChange -= StateHasChanged;
    }

}
